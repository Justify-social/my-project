{
  "name": "my-project",
  "version": "0.1.0",
  "private": true,
  "description": "Next.js application with comprehensive UI component system, FontAwesome icons, and robust testing",
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "scripts": {
    "dev": "next dev --turbo",
    "build": "node scripts/icons/generate-registry-module.mjs && next build",
    "build:registry": "tsx scripts/ui/generate-component-registry.ts",
    "start": "next start",
    "lint": "eslint --config eslint.config.mjs --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit",
    "test": "run-p test:unit test:integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=tests/unit --passWithNoTests",
    "test:integration": "jest --testPathPattern=tests/integration --passWithNoTests",
    "test:e2e": "cypress run",
    "test:local": "jest --testPathPattern=src",
    "lint:all": "run-p lint:check lint:format",
    "format": "prettier --config config/prettier/.prettierrc.json --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --config config/prettier/.prettierrc.json --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "lint-staged": "lint-staged",
    "icons": "run-s icons:download icons:check",
    "icons:download": "run-p icons:download:*",
    "icons:check": "node scripts/icons/audit-icons.mjs",
    "icons:lock": "bash scripts/icons/lock-registry-files.sh",
    "icons:unlock": "sudo chflags noschg public/static/*-icon-registry.json && chmod 644 public/static/*-icon-registry.json",
    "icons:validate:ssot": "node scripts/icons/validate-id-ssot.mjs",
    "icons:validate:registry": "node scripts/icons/validate-icon-registry.js",
    "icons:fix:imports": "bash scripts/icons/fix-icon-imports.sh",
    "icons:archive": "bash scripts/icons/archive-registry-backups.sh",
    "icons:render:test": "node scripts/icons/verify-icon-rendering.mjs",
    "icons:merge": "node scripts/icons/merge-icon-registries.mjs",
    "config": "run-p config:organize config:migrate",
    "config:organize": "node config/scripts/config-organizer.mjs",
    "config:migrate": "node config/scripts/migrate-config.mjs",
    "validate:config": "node config/scripts/validate-config.js",
    "algolia": "run-s algolia:index algolia:reindex",
    "algolia:index": "ts-node scripts/campaign/index-sample-campaigns.ts",
    "algolia:reindex": "curl -X GET http://localhost:3000/api/search/index-campaigns",
    "cypress": "cypress run",
    "cypress:open": "cypress open",
    "analyze": "ANALYZE=true next build",
    "create-test-fixtures": "node --loader ts-node/esm cypress/fixtures/create-test-image.js",
    "postinstall": "prisma generate",
    "generate:campaign": "prisma generate --schema=./prisma/schemas/campaign-wizard.prisma",
    "migrate:campaign": "prisma migrate dev --schema=./prisma/schemas/campaign-wizard.prisma --name init_campaign_wizard",
    "prepare": "husky",
    "index-campaigns": "node scripts/campaign/index-campaigns.js",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:parallel": "cypress run --config-file config/cypress/cypress-parallel.config.js --record --parallel",
    "cy:run:parallel:local": "npm run cy:run:parallel:container1 & npm run cy:run:parallel:container2 & npm run cy:run:parallel:container3 & npm run cy:run:parallel:container4 & wait",
    "cy:run:parallel:container1": "CONTAINER_ID=1 cypress run --config-file config/cypress/cypress-parallel.config.js",
    "cy:run:parallel:container2": "CONTAINER_ID=2 cypress run --config-file config/cypress/cypress-parallel.config.js",
    "cy:run:parallel:container3": "CONTAINER_ID=3 cypress run --config-file config/cypress/cypress-parallel.config.js",
    "cy:run:parallel:container4": "CONTAINER_ID=4 cypress run --config-file config/cypress/cypress-parallel.config.js",
    "cy:run:headed": "cypress run --headed",
    "cy:test": "start-server-and-test start http://localhost:3000 cy:run",
    "cy:test:parallel": "start-server-and-test start http://localhost:3000 cy:run:parallel:local",
    "cy:report": "bash scripts/cypress/cypress-report.sh",
    "cy:report:aggregate": "node config/cypress/scripts/aggregate-parallel-results.js",
    "cy:report:html": "node config/cypress/scripts/generate-html-report.js",
    "cy:report:complete": "npm run cy:report:aggregate && npm run cy:report:html",
    "cy:optimize": "node config/cypress/scripts/optimize-test-execution.js",
    "cy:analyze": "npm run cy:optimize && npm run cy:report:complete"
  },
